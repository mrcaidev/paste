{
  "*.scss": [
    "stylelint --fix --cache --cache-location ./node_modules/.cache/stylelint/.stylelint-cache",
    "prettier --write --cache"
  ],
  "*.{js,ts,jsx,tsx}": [
    "eslint --fix --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache",
    "prettier --write --cache"
  ],
  "*.{html,json,md,yaml}": "prettier --write --cache"
}
